<odoo>
       <data>
            <record id="ir_cron_check_server_status" model="ir.cron">
                <field name="name">Check Server Status</field>
                <field name="model_id" ref="model_server"/>
                <field name="state">code</field>
                <field name="code">model._check_server_status_cron()</field>
                <field name="interval_number">1</field>
                <field name="interval_type">hours</field>
                <field name="numbercall">-1</field>
            </record>

             <record id="ir_cron_check_service_status" model="ir.cron">
                <field name="name">Check Service Status</field>
                <field name="model_id" ref="model_service"/>
                <field name="state">code</field>
                <field name="code">model._check_service_status_cron()</field>
                <field name="interval_number">1</field>
                <field name="interval_type">hours</field>
                <field name="numbercall">-1</field>
                <field name="active">True</field>
            </record>
     </data>
</odoo>



